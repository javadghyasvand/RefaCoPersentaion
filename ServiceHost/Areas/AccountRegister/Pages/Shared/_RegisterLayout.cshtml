@using GeneralManagement.Application.Contracts.SiteLogo

@using Microsoft.AspNetCore.Mvc.TagHelpers
<!DOCTYPE html>
<html lang="en">
@inject ISiteLogoApplication SiteLogoApplication
@{
    var logo = SiteLogoApplication.GetSiteLogo();
}
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="">
    <meta name="keyword" content="">
    <link rel="shortcut icon" href="~/Images/@logo.Picture">
    <title> @logo.Title || @ViewData["Title"]</title>

    <link href="~/AdminTheme/assets/css/bootstrap.min.css" rel="stylesheet" />

    <link href="~/AdminTheme/assets/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/AdminTheme/assets/ionicon/css/ionicons.min.css" rel="stylesheet" />
    <link href="~/AdminTheme/assets/css/material-design-iconic-font.min.css" rel="stylesheet">

    <link href="~/AdminTheme/assets/css/animate.css" rel="stylesheet" />

    <link href="~/AdminTheme/assets/css/waves-effect.css" rel="stylesheet">


    <link href="~/AdminTheme/assets/sweet-alert/sweet-alert.min.css" rel="stylesheet">


    <link href="~/AdminTheme/assets/css/helper.css" rel="stylesheet" type="text/css" />
    <link href="~/AdminTheme/assets/css/style.css" rel="stylesheet" type="text/css" />

    <script src="~/AdminTheme/assets/js/modernizr.min.js"></script>


    <link href="~/AdminTheme/assets/notifications/notification.css" rel="stylesheet" />
    @await RenderSectionAsync("Css", required: false)
</head>

<body>


    <div class="wrapper-page">
        @RenderBody()
    </div>

    <script src="~/AdminTheme/assets/js/jquery.min.js" type="text/javascript"></script>
    <script src="~/AdminTheme/assets/js/bootstrap.min.js" type="text/javascript"></script>

    <script src="~/AdminTheme/assets/js/waves.js" type="text/javascript"></script>
    <script src="~/AdminTheme/assets/js/wow.min.js" type="text/javascript"></script>
    <script src="~/AdminTheme/assets/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="~/AdminTheme/assets/js/jquery.scrollTo.min.js" type="text/javascript"></script>
    <script src="~/AdminTheme/assets/chat/moment-2.2.1.js" type="text/javascript"></script>
    <script src="~/AdminTheme/assets/jquery-sparkline/jquery.sparkline.min.js" type="text/javascript"></script>
    <script src="~/AdminTheme/assets/jquery-detectmobile/detect.js" type="text/javascript"></script>
    <script src="~/AdminTheme/assets/fastclick/fastclick.js" type="text/javascript"></script>
    <script src="~/AdminTheme/assets/jquery-slimscroll/jquery.slimscroll.js" type="text/javascript"></script>
    <script src="~/AdminTheme/assets/jquery-blockui/jquery.blockUI.js" type="text/javascript"></script>

    <script src="~/AdminTheme/assets/sweet-alert/sweet-alert.min.js" type="text/javascript"></script>
    <script src="~/AdminTheme/assets/sweet-alert/sweet-alert.init.js" type="text/javascript"></script>


    <script src="~/AdminTheme/assets/js/jquery.app.js" type="text/javascript"></script>

    <script src="~/AdminTheme/assets/js/jquery.dashboard.js" type="text/javascript"></script>

    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js" type="text/javascript"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>

    <script src="~/AdminTheme/assets/js/site.js" type="text/javascript"></script>

    @await RenderSectionAsync("Scripts", required: false)


    <script src="~/AdminTheme/assets/notifications/notify.min.js"></script>
    <script src="~/AdminTheme/assets/notifications/notify-metro.js"></script>
    <script src="~/AdminTheme/assets/notifications/notifications.js"></script>
    <script src="~/AdminTheme/assets/js/jquery.app.js"></script>

</body>
</html>